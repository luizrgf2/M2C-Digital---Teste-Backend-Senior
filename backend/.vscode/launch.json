{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to NestJS",
        "port": 9229,
        "restart": true,
        "protocol": "inspector",
        "timeout": 10000,
        "autoAttachChildProcesses": true,
        "sourceMaps": true,
        "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "skipFiles": ["<node_internals>/**", "node_modules/**"]
      }
    ]
  }
  